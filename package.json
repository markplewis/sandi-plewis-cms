    "debug": "Gathers information on Sanity environment"
  },
  "scripts": {
    "dev": "sanity dev",
    "start": "sanity start",
    "build": "sanity build",
    "deploy": "sanity deploy",
    "debug": "sanity debug"
  },
  "dependencies": {
    "@sanity/color-input": "^3.0.1",
    "@sanity/icons": "^2.1.0",
    "@sanity/vision": "^3.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.7.1",
    "react-is": "^18.2.0",
    "sanity": "^3.0.0",
    "sanity-plugin-asset-source-unsplash": "^1.0.1",
    "styled-components": "^5.2.0"
  },
  "devDependencies": {
