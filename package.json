{
  "name": "sandiplewis-cms",
  "version": "1.0.0",
  "private": true,
  "description": "CMS for Sandi Plewis' blog website",
  "homepage": "https://admin.sandiplewis.com",
  "author": "Mark Plewis <mplewis1@gmail.com>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/markplewis/sandiplewis-cms.git"
  },
  "license": "UNLICENSED",
  "main": "package.json",
  "keywords": [
    "sanity"
  ],
  "prettier": {
    "arrowParens": "avoid",
    "bracketSameLine": true,
    "jsxSingleQuote": false,
    "printWidth": 100,
    "semi": true,
    "singleQuote": false,
    "tabWidth": 2,
    "trailingComma": "none"
